{
  "$schema": "../../../schema/test.json",
  "title": "Disclosure widget (show/hide)",
  "description": "This example demonstrates how the disclosure design pattern can be used to create a show/hide button.",
  "html_file": "aria/aria-expanded.html",
  "assertions": [
    {
      "feature_id": "html/button_element",
      "feature_assertion_id": "convey_name"
    },
    {
      "feature_id": "html/button_element",
      "feature_assertion_id": "convey_role"
    },
    {
      "feature_id": "aria/aria-expanded_attribute",
      "feature_assertion_id": "convey_value_false",
      "applied_to": "html/button_element"
    },
    {
      "feature_id": "aria/aria-expanded_attribute",
      "feature_assertion_id": "convey_value_true",
      "applied_to": "html/button_element"
    },
    {
      "feature_id": "aria/aria-expanded_attribute",
      "feature_assertion_id": "convey_change_in_value",
      "applied_to": "html/button_element"
    }
  ],
  "commands": {
    "jaws": {
      "chrome": [
        {
          "command": "next_focusable_item",
          "css_target": "button[aria-expanded=\"false\"]",
          "before": {
            "mode": "auto",
            "focus_location": "before target",
            "virtual_location": "before target"
          },
          "after": "target",
          "notes": "tab to the button with it collapsed",
          "output": "\"show more button, collapsed\"",
          "results": [
            {
              "feature_id": "html/button_element",
              "feature_assertion_id": "convey_name",
              "result": "pass",
              "notes": null
            },
