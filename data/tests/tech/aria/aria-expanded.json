              "feature_assertion_id": "convey_role",
              "result": "pass",
              "notes": null
            },
            {
              "feature_id": "aria/aria-expanded_attribute",
              "feature_assertion_id": "convey_value_false",
              "applied_to": "html/button_element",
              "result": "pass",
              "notes": null
            }
          ]
        },
        {
          "command": "next_focusable_item",
          "css_target": "button[aria-expanded=\"true\"]",
          "before": {
            "mode": "auto",
            "focus_location": "before target",
            "virtual_location": "before target"
          },
          "after": "target",
          "notes": "tab to the button with it expanded",
          "output": "\"show more button, expanded\"",
          "results": [
            {
              "feature_id": "aria/aria-expanded_attribute",
              "feature_assertion_id": "convey_value_true",
              "applied_to": "html/button_element",
              "result": "pass",
              "notes": null
            }
          ]
        },
        {
          "command": "activate_button",
          "css_target": "button[aria-expanded]",
          "before": {
            "mode": "auto",
            "focus_location": "target",
            "virtual_location": "target"
          },
          "after": "target",
          "output": "\"expanded\" or \"collapsed\"",
          "results": [
            {
              "feature_id": "aria/aria-expanded_attribute",
              "feature_assertion_id": "convey_change_in_value",
              "applied_to": "html/button_element",
              "result": "partial",
              "notes": "only announced the new state on the first activation after tabbing to the element. subsequent activations only announce \"to activate press enter\""
