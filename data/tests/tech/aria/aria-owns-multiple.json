{
  "$schema": "../../../schema/test.json",
  "title": "aria-owns multiple reference",
  "description": "Tests aria-owns with multiples reference to other elements.",
  "html_file": "aria/aria-owns-multiple.html",
  "assertions": [
    {
      "feature_id": "aria/aria-owns_attribute",
      "feature_assertion_id": "convey_multiple_relationships",
      "applied_to": "html/li_element",
      "references": ["html/ul_element", "html/ul_element"]
    },
    {
      "feature_id": "aria/aria-owns_attribute",
      "feature_assertion_id": "order_after_existing_children",
      "applied_to": "html/li_element",
      "references": ["html/ul_element", "html/ul_element"]
    }
  ],
  "procedures": [
    {
      "key": "navigate_through_list",
      "title": "navigate through the list",
      "css_target": "#target",
      "steps": [
        {
          "action": "find target"
        },
        {
          "action": "set mode to",
          "value": "auto"
        },
        {
          "action": "issue command",
          "summary": "navigate forward to the start of the list",
          "command": "next_item",
          "ensure_at_location": {
            "virtual": "start of selector",
            "focus": "before selector"
          }
        },
        {
          "action": "issue command",
          "multiple": "true",
          "observe": "both of the fruits sub-lists are included as children of the target list and are not duplicated elsewhere on the page",
          "ensure_at_location": {
            "virtual": "after target",
            "focus": "before target"
          }
        }
      ]
    }
  ],
  "commands": {
