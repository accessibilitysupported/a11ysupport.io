  "title": "aria-label test",
  "description": "A simple aria-label test",
  "html_file": "aria/aria-labelling.html",
  "assertions": [
    {
      "feature_id": "aria/aria-label_attribute",
      "feature_assertion_id": "contribute_to_accessible_name",
      "applied_to": "html/button_element"
    }
  ],
  "commands": {
    "dragon_win": {
      "chrome": [
        {
          "command": "activate_name",
          "css_target": "*[aria-label]",
          "before": {
            "mode": "auto",
            "virtual_location": "na",
            "focus_location": "na"
          },
          "after": "na",
          "output": "the correct button was activated",
          "notes": "said \"click target 1\"",
          "results": [
            {
              "feature_id": "aria/aria-label_attribute",
              "feature_assertion_id": "contribute_to_accessible_name",
              "applied_to": "html/button_element",
