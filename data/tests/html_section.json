  "title": "HTML section element with no accessible name",
  "description": "This section has no accessible name per HTML AAM 5.15.1 Section and Grouping Element Accessible Name Computation. It will not be mapped to a role and should be considered presentational.",
  "assertions": [
    {
      "feature_id": "html/section_element",
      "feature_assertion_id": "no_role_if_not_named",
      "css_target": "#target"
    }
  ],
  "commands": {
    "dragon_win": {},
    "jaws": {
      "chrome": [
        {
          "command": "next_item",
          "css_target": "section:not([aria-label])",
          "before": {
            "mode": "auto",
            "virtual_location": "before target",
            "focus_location": "before target"
          },
          "after": "target",
          "output": "\"sample text line 1\"",
          "results": [
            {
              "feature_id": "html/section_element",
